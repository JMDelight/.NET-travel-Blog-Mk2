@{
    ViewData["Title"] = "Travel Blog | Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<TravelBlog.Models.Experience>

<ul>
    @foreach(var listItem in Model)
    {
    <li>@Html.DisplayFor(modelItem => listItem.Description) - @Html.ActionLink(listItem.Location.Name, "../Locations/Details", new { id = listItem.LocationId })</li>
   
    }
</ul>