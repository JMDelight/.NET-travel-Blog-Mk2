@{
    ViewData["Title"] = "Travel Blog | Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<TravelBlog.Models.Experience>

<ul>
    @foreach(var listItem in Model)
    {
    <li>@Html.DisplayFor(modelItem => listItem.Description) - @Html.DisplayFor(modelItem => listItem.Location.Name)</li>
    }
</ul>